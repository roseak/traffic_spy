<div class = 'containter well'>
  <h2>Total Received: <%= params[:total] %></h2>
</div>

<div class = 'panel panel-default'>
  <table class="table">
    <thead>
      <tr>
        <th>Time</th>
        <th>Count</th>
      </tr>
    </thead>
    <tbody>
      <% params[:data].each do |time, count| %>
        <tr>
          <td><%= time %></td>
          <td><%= count %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
