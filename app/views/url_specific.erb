<div class="panel panel-default">
  <div class="panel-heading">Response Time Statistics</div>
  <table class="table">
    <thead>
      <tr>
        <th>Response Time</th>
        <th>Miliseconds</th>
      </tr>
    </thead>
    <tbody>
      <% @params[:response].each do |stat, time| %>
        <tr>
          <td><%= stat %></td>
          <td><%= time %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
