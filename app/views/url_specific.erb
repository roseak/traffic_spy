<div class="panel panel-default">
  <div class="panel-heading">Response Time Statistics</div>
  <table class="table">
    <tbody>
        <tr>
          <td>Shortest Response Time</td>
          <td><%=@params[:response]["min"]%></td>
        </tr>
        <tr>
          <td>Longest Response Time</td>
          <td><%=@params[:response]["max"]%></td>
        </tr>
        <tr>
          <td>Average Response Time</td>
          <td><%=@params[:response]["avg"]%></td>
        </tr>
    </tbody>
  </table>
</div>
<% @params[:data].each do |contents| %>
<% table = contents.last %>
  <div class="panel panel-default">
    <div class="panel-heading"><%= table[:title] %></div>
    <table class="table">
      <thead>
        <tr>
          <th><%= table[:headers].first %></th>
          <th><%= table[:headers].last %></th>
        </tr>
      </thead>
      <tbody>
        <% table[:rows].each do |label, value| %>
          <tr>
            <td><%= label %></td>
            <td><%= value %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
<% end %>
